{
  "name": "imohash",
  "description": "Fast hashing for large files",
  "keywords": [
    "hashing",
    "hash"
  ],
  "repository": "https://github.com/clockwork-dog/imohash",
  "homepage": "https://github.com/clockwork-dog/imohash",
  "author": "Matt Oakes <hello@mattoakes.net>",
  "license": "MIT",
  "version": "1.0.1",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "yarn run build --watch",
    "types": "tsc",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "yarn npm publish --access public"
  },
  "dependencies": {
    "murmurhash3js-revisited": "^3.0.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/murmurhash3js-revisited": "^3.0.3",
    "@types/node": "^22.12.0",
    "@types/varint": "^6.0.3",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "memfs": "^4.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.4"
  },
  "dependenciesMeta": {
    "murmurhash3-ts": {
      "built": false
    }
  }
}
